
<tr>
  <td class="table-thumbnail">
      <%= image_tag account_row.thumbnail %>
  </td>
  <td>
  	<div rel="tooltip" title="<%=account_row.name%>">
  	<%=link_to(truncate(h(account_row.name), :length=>50, :separator => ' ', :omission=>"..."), account_row)%>
	</div>
  </td>
  <td><%=Country.find_ex(account_row.country_id).i18n_name%></td>
  <td><%=link_to(account_row.website, "http://" + account_row.website, :target => '_blank') if account_row.website%>
  </td>
</tr>